<!DOCTYPE html>
<html>
    <body>
        <h1>Проекты</h1>
        <form action="/api/create_project" method="post">
            <table>
                <tr>
                    <td><label>Название:</label></td>
                    <td><input type="text" name="title" /></td>
                </tr>
            </table>
            <input type="submit" value="Создать проект" />
        </form>
        <ul>
            {% for project in user.projects %}
                <li>
                    <a href="/projects/{{ project.id }}">{{ project.title }}</a>
                </li>
            {% endfor %}
        </ul>
    </body>
</html>
